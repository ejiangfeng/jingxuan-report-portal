{
  "name": "jingxuan-report-backend",
  "version": "1.0.0",
  "description": "鲸选部门数据查询报表平台后端服务",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t jingxuan/report-backend:latest .",
    "docker:run": "docker run -p 4000:4000 --env-file .env jingxuan/report-backend:latest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "report",
    "analytics",
    "oceanbase",
    "dataworks",
    "express"
  ],
  "author": "鲸选技术团队",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "mysql2": "^3.2.0",
    "exceljs": "^4.4.0",
    "moment": "^2.29.4",
    "winston": "^3.10.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "archiver": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "node-cron": "^3.0.3",
    "express-async-handler": "^1.2.0",
    "node-fetch": "^3.3.1",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/mysql": "^2.15.21",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/express-rate-limit": "^5.0.0",
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.4",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "prettier": "^3.0.0",
    "@types/supertest": "^2.0.12"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
